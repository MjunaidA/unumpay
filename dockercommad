aws ecr get-login-password --region us-east-1 --profile alche_ecw | docker login --username AWS --password-stdin 599938016587.dkr.ecr.us-east-1.amazonaws.com

git add --all && git commit -m 'main.js updated..' && git push origin master
docker build -t shopify-unified-transaction-frontend .
docker tag shopify-unified-transaction-frontend:latest 599938016587.dkr.ecr.us-east-1.amazonaws.com/shopify-unified-transaction-frontend:latest
docker push 599938016587.dkr.ecr.us-east-1.amazonaws.com/shopify-unified-transaction-frontend:latest
aws ecs update-service  --service shopify-unified-transaction-frontend --cluster unumpay-cluster --force-new-deployment --region us-east-1 --profile alche_ecw

docker run -p 8080:8080 shopify-unified-transaction-frontend